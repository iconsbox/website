  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build && gh-pages -d public -r https://$GH_TOKEN@github.com/iconsbox/website",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
